<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ponduri.cardboarddemo" >


    <application
        android:allowBackup="true"
        android:icon="@drawable/rsg"
        android:largeHeap="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VideoScreen"
            android:label="@string/title_activity_video_screen"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ImageScreen"
            android:screenOrientation="landscape"
            android:label="@string/title_activity_image_screen" >
        </activity>
    </application>

</manifest>
